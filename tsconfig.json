{
  "compilerOptions": {
    "target": "ES2020",
    "module": "es2020",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "allowImportingTsExtensions": true,
    "declaration": true
  }
}
